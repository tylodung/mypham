  },
  "private": true,
  "engines": {
    "node": "10.4.1",
    "npm": "6.1.0"
  },
  "dependencies": {
    "@material-ui/core": "1.2.1",
    "accepts": "1.3.5",
    "bluebird": "3.5.1",
    "cached-request": "1.1.4",
    "classnames": "2.2.6",
    "convert-svg-to-png": "0.4.0",
    "dotenv": "6.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "express-winston": "2.5.1",
    "fetch-jsonp": "1.1.3",
    "file-saver": "1.3.8",
    "font-awesome": "^4.7.0",
    "fs-extra": "6.0.1",
    "glob": "7.1.2",
    "gm": "1.23.1",
    "graphql": "0.12.3",
    "graphql-request": "1.6.0",
    "graphql-tag": "2.6.1",
    "grid-styled": "4.1.1",
    "html-pdf": "2.2.0",
    "image-size": "0.6.3",
    "image-to-ascii": "3.0.11",
    "intl": "1.2.5",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "lru-cache": "4.1.3",
    "markdown-loader": "3.0.0",
    "moment": "2.22.2",
    "moment-timezone": "0.5.17",
    "newrelic": "4.1.5",
    "next": "6.0.3",
    "next-routes": "1.4.2",
    "node-fetch": "2.1.2",
    "payment": "2.3.0",
    "postcss-nested": "3.0.0",
    "prop-types": "15.6.1",
    "qrcode.react": "0.8.0",
    "raw-loader": "0.5.1",
    "rc-table": "6.2.2",
    "react": "16.4.1",
    "react-add-to-calendar": "0.1.5",
    "react-apollo": "1.4.16",
    "react-bootstrap": "0.32.1",
    "react-datetime": "2.14.0",
    "react-dom": "16.4.1",
    "react-dropzone": "4.2.11",
    "react-geosuggest": "2.7.0",
    "react-gmaps": "1.9.0",
    "react-intl": "2.4.0",
    "react-markdown": "3.3.2",
    "react-mde": "git://github.com/xdamman/react-mde.git#npm",
    "react-quill": "1.2.7",
    "react-scrollchor": "5.1.0",
    "react-stickynode": "2.0.1",
    "request": "2.87.0",
    "showdown": "^1.8.6",
    "slugify": "1.3.0",
    "styled-components": "3.3.2",
    "styled-jsx": "2.2.6",
    "styled-jsx-plugin-postcss": "0.1.3",
    "universal-analytics": "0.4.17",
    "webpack": "3.10.0",
    "winston": "3.0.0"
  },
  "scripts": {
    "start": "node build/server",
    "dev": "babel-node src/server",
    "build": "npm run build:server && next build build",
    "test": "npm run test:src && npm run test:e2e",
    "test:src": "TZ=UTC jest src/*",
    "test:server": "TZ=UTC jest test/server/*",
    "test:e2e": "TZ=UTC ./scripts/run_e2e_tests.sh",
    "build:server": "babel --copy-files ./src --out-dir ./build --ignore src/templates/widget.js,src/**/__tests__/*",
    "build:langs": "rm -rf build/messages && babel src/**/*.js > /dev/null && babel-node scripts/translate.js",
    "deploy:production": "git push production master",
    "deploy:staging": "git push -f staging master && heroku ps:scale web=1 -a opencollective-staging-api && heroku ps:scale web=1 -a oc-staging-frontend && heroku ps:scale web=1 -a opencollective-staging-website",
    "stop:staging": "heroku ps:scale web=0 -a oc-staging-frontend && heroku ps:scale web=0 -a opencollective-staging-website && heroku ps:scale web=0 -a opencollective-staging-api",
    "git:clean": "./scripts/git_clean.sh",
    "postinstall": "if test \"$NODE_ENV\" = \"\" || test \"$NODE_ENV\" = \"development\" ; then echo \"Skipping postinstall build because NODE_ENV is '${NODE_ENV}'\" ; else npm run build ; fi",
    "hint": "npm run lint",
    "lint": "eslint '**/*.js' --quiet",
    "lint:fix": "eslint '**/*.js' --fix",
    "pretest": "npm run lint",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "cypress": "TZ=UTC cypress run"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/node": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/preset-stage-0": "7.0.0-beta.42",
    "@babel/runtime": "7.0.0-beta.42",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "23.0.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-styled-components": "^1.5.1",
    "commitizen": "^2.10.1",
    "cypress": "3.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-opencollective": "^1.2.1",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "jest": "23.1.0",
    "jest-styled-components": "^5.0.1",
    "jsdom": "^11.11.0",
    "mkdirp": "^0.5.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.4.1",
    "regenerator-runtime": "^0.11.1",
    "semantic-release": "^15.5.2",
    "sinon": "6.0.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/mocks/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "publish": [
      "@semantic-release/github"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "@babel/cli",
      "@babel/core",
      "@babel/node",
      "@babel/preset-env",
      "@babel/preset-stage-0",
      "@babel/runtime",
      "graphql",
      "graphql-tag",
      "react-apollo",
      "react-mde",
      "webpack"
    ]
  },
  "cacheDirectories": [
    ".cache",
    "node_modules"
  ]
}